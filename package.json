{
  "name": "animalese-typing",
  "productName": "Animalese Typing",
  "version": "0.0.1",
  "main": "main.js",
  "author": {
    "name": "joshxviii",
    "email": "dagexviii.dev@gmail.com"
  },
  "keywords": [],
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "com.joshxviii.animalese-typing",
    "productName": "Animalese Typing",
    "files": [
      "**/*"
    ],
    "directories": {
      "output": "exports"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/images/icon256.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/images/icon256.png"
    }
  },
  "license": "MIT",
  "description": "\"Animalese Typing Desktop\"",
  "repository": "https://github.com/joshxviii/animalese-typing-desktop",
  "dependencies": {
    "active-win": "^8.2.1",
    "electron-fetch": "^1.9.1",
    "electron-log": "^4.4.8",
    "electron-store": "^6.0.1",
    "howler": "^2.2.4",
    "iohook": "^0.9.3"
  },
  "devDependencies": {
    "electron": "^12.2.3",
    "electron-builder": "~24.13.3"
  },
  "iohook": {
    "targets": [
      "electron-87"
    ],
    "platforms": [
      "win32",
      "darwin",
      "linux"
    ],
    "arches": [
      "x64"
    ]
  }
}
